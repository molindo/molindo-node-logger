{
  "name": "molindo-node-logger",
  "version": "1.3.0",
  "description": "A node.js logger that integrates well with the Molindo infrastructure.",
  "main": "lib/index.js",
  "repository": "https://github.com/molindo/molindo-node-logger.git",
  "author": "Jan Amann <jam@molindo.at>",
  "license": "MIT",
  "keywords": [
    "node",
    "logger",
    "logging",
    "winston"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "yarn run lint && yarn run test -- --run && bunchee",
    "lint": "eslint src",
    "test": "vitest",
    "test:watch": "npm run test -- --watch --onlyChanged",
    "prepublish": "npm run lint && npm run test && rimraf ./lib && npm run build"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "babel-jest": "21.2.0",
    "bunchee": "^5.1.2",
    "eslint": "8.56.0",
    "eslint-config-molindo": "7.0.0",
    "prettier": "3.2.5",
    "supertest": "6.3.4",
    "vitest": "^1.3.1",
    "vitest-mock-process": "^1.0.4"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "winston": "^3.13.0",
    "winston-transport": "^4.7.0"
  },
  "engines": {
    "node": ">= 20"
  }
}
