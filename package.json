    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "eslint": "8.56.0",
    "eslint-config-molindo": "7.0.0",
    "jest": "21.2.1",
    "prettier": "3.2.5",
    "supertest": "3.0.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.19.2",
    "express-winston": "^2.4.0",
    "winston": "^2.4.0"
